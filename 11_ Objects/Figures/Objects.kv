Canvas4:
#:set tam dp(150)
<Canvas1@Widget>:
    canvas:
        Color:
            rgb: 1,0.5,1
        Rectangle:
            id: rec
            pos: dp(250)/2,self.center_y-dp(150)/2
            size: dp(130),dp(70)

            Ellipse:
                pos: self.width/4, self.height/1.2
                size: tam, tam/2

            Line:
                points: (0,0,self.width/4,100,self.width/2,0,self.width*3/4,100,self.width,0)
                width: 5
<Canvas2@Widget>:
    canvas:
        Line:
            points: (self.width/2,0,self.width/2,self.height)
            width: 2
        Line:
            points: (0,self.height/2,self.width,self.height/2)
            width: 2
<Canvas3@Widget>:
    canvas:
        Color:
            rgba: 1,1,1,0.6

        Line:
            circle: (self.width/8,self.height/8,self.width/8) #    center_x, center_y, radius
            width: 2
        # Line:
        #     circle: (self.width/2,self.height/2,self.width/10,self.height/7) #    center_x, center_y, radius_x, radius_y
        #     width: 4
        Color:
            rgb: 1,0.5,1
        Line:
            rectangle: (dp(20),self.height/8,self.width/8,self.height/8)
            width: 2
<Canvas4>:
    Button:
        pos: 100,400
        text: "Move Rectangle"
        on_press: root.button_click()